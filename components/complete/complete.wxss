/* components/complete/complete.wxss */
.complete{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 30%;
}
.complete-img{
  width: 260rpx;
  height: 230rpx;
}
.complete-status{
  margin: 36rpx 0;
  font-size: 46rpx;
}
.complete-confirm{
  font-size: 30rpx;
  color: #666;
}
.complete-btn{
  width: 290rpx;
  height: 110rpx;
  margin-top: 80rpx;
  border-radius:10rpx; 
  font-size: 36rpx;
  color: #fff;
  text-align: center;
  line-height: 110rpx;
  background-color: #E93A3D;
}