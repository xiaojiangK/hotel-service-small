/* pages/orderList/orderList.wxss */
page{
  height: 100%;
  background-color: #f1f1f1;
}
.complete{
  width: 100%;
  height: 100%;
  display: flex;
  background: #fff;
  flex-direction: column;
  align-items: center;
}
.complete-img{
  width: 260rpx;
  height: 230rpx;
  margin-top: 30%;
}
.complete-status{
  margin: 36rpx 0;
  font-size: 46rpx;
}
.complete-confirm{
  margin-top: 50rpx;
  font-size: 30rpx;
  color: #666;
}
.no-order{
  width: 100%;
  height: 100rpx;
  color: #999;
  font-size: 28rpx;
  text-align: center;
  line-height: 100rpx;
}