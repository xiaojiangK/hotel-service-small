page {
  height: 100%;
  background: #F2F2F2;
}
.top {
  width: 100%;
  height: 734rpx;
  background: white;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 20rpx;
}
.img-warning {
  width: 196rpx;
  height: 146rpx;
  margin-top: 108rpx;
}
.text {
  font-size: 32rpx;
  color: #333333;
  margin-top: 112rpx;
}
.text::after {
  border-bottom: 1rpx solid #E1E1E1;
}
.bold {
  font-weight: bold;
}
.msg {
  font-size: 24rpx;
  color: #666666;
  margin-top: 20rpx;
}
.wifi-ssid {
  width: 90%;
  height: 40rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 50rpx;
}
.icon-wifi {
  width: 40rpx;
  height: 40rpx;
}
.wifi-text {
  font-size: 32rpx;
  color: #494A4A;
  margin-left: 50rpx;
}
.password {
  width: 350rpx;
}
.btn-copy {
  width: 180rpx;
  height: 58rpx;
  background: #4A90E3;
  line-height: 58rpx;
  text-align: center;
  color: white;
  font-size: 28rpx;
  border-radius: 8rpx;
  margin-left: 56rpx;
  border: none;
}
.bottom {
  width: 100%;
  height: 220rpx;
  background: white;
  padding-top: 50rpx;
}
.mt {
  margin-top: 18rpx;
}